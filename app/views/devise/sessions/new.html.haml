.new-content
  .sessions-new
    .logo-search
      %a{href:"/"}
        = image_tag 'logo2.png'
  .new-login
    .font
    アカウントをお持ちでない方はこちら
    .link-to-new
      =link_to "新規会員登録",new_user_path,method: :get,class: "link-nav"
  
  .second-login  
    .link-facebook
      %i.fab.fa-facebook-square.facebook-icon-1
      =link_to "Facebookで登録"

    .link-google
      =image_tag 'google.png'
      =link_to "Googleで登録"
    = form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
      .field-4
        = f.email_field :email, autofocus: true, autocomplete: "email",placeholder:'メールアドレス',class: "e-mail"
      .field-5
        = f.password_field :password, autocomplete: "current-password",placeholder:'パスワード',class: "password"
      .lobot
        .lobot-talk
          私はロボットではあり
          %br/
          ません
      .actions
        = f.submit "ログイン",class: "log-in-btn"
    = render "devise/shared/links",class: "no-password"
